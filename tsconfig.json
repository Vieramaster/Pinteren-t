{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "types": ["node"],
    "lib": ["ES2022", "DOM"],
    "allowJs": false,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,

    "esModuleInterop": true
  },
  "include": ["src"]
}
